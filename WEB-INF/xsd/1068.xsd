<?xml version="1.0" encoding="ISO-8859-1"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">	<xs:element name="facturas">		<xs:annotation>			<xs:documentation>Elemento que encapsula la información del formulario 1068 versión 6: INFORME DE DATOS DE FACTURAS COMERCIALES Y DOCUMENTOS DE TRANSPORTE POR COMPRAS DE MERCANCÍAS AL RESTO DEL MUNDO POR USUARIO INSTALADO EN ZONA FRANCA INDUSTRIAL DE BIENES</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element ref="datos" minOccurs="0" maxOccurs="unbounded"/>			</xs:sequence>			<xs:attribute name="czf" use="required">				<xs:annotation>					<xs:documentation>Código Zona Franca</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:int">						<xs:minInclusive value="0"/>						<xs:maxInclusive value="99999"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="raz" use="required">				<xs:annotation>					<xs:documentation>Razón Social</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:minLength value="0"/>						<xs:maxLength value="450"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="nfac" use="required">				<xs:annotation>					<xs:documentation>Número factura por compra de mercancías que se introducen a Zona Franca del resto del mundo</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:minLength value="0"/>						<xs:maxLength value="20"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="ffac" type="xs:date" use="required">				<xs:annotation>					<xs:documentation>Fecha factura por compra de mercancías que  se introducen a Zona Franca del resto del mundo</xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="cmon" use="required">				<xs:annotation>					<xs:documentation>Código moneda negociación (factura por compra de mercancías que se introducen a Zona Franca del resto del mundo) </xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:minLength value="0"/>						<xs:maxLength value="3"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>			<xs:attribute name="val" use="required">				<xs:annotation>					<xs:documentation>Valor en moneda de negociación de la factura por compra de mercancías que se introducen a Zona Franca del resto del mundo.</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:decimal">						<xs:totalDigits value="22"/>						<xs:fractionDigits value="2"/>						<xs:maxInclusive value="99999999999999999999.99"/>					</xs:restriction>				</xs:simpleType>			</xs:attribute>		</xs:complexType>	</xs:element>	<xs:element name="mas">		<xs:annotation>			<xs:documentation>Elemento raiz del xml de carga masiva</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="Cab" type="CabType"/>				<xs:element ref="facturas" maxOccurs="unbounded"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:complexType name="CabType">		<xs:sequence>			<xs:element name="Ano" type="xs:gYear" nillable="false">				<xs:annotation>					<xs:documentation>Año de envio</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="CodCpt" nillable="false">				<xs:annotation>					<xs:documentation>Concepto</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:int">						<xs:minInclusive value="0"/>						<xs:maxInclusive value="99"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="Formato" fixed="1068">				<xs:simpleType>					<xs:restriction base="xs:positiveInteger"/>				</xs:simpleType>			</xs:element>			<xs:element name="Version" type="xs:positiveInteger" fixed="6"/>			<xs:element name="NumEnvio">				<xs:annotation>					<xs:documentation>Numero de envio</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:positiveInteger">						<xs:totalDigits value="8"/>						<xs:maxInclusive value="99999999"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="FecEnvio" type="xs:dateTime">				<xs:annotation>					<xs:documentation>Fecha de envio</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="FecInicial" type="xs:date">				<xs:annotation>					<xs:documentation>Fecha inicial</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="FecFinal" type="xs:date">				<xs:annotation>					<xs:documentation>Fecha Final</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="ValorTotal" type="xs:double">				<xs:annotation>					<xs:documentation>Valor Total</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="CantReg">				<xs:annotation>					<xs:documentation>Cantidad de registros</xs:documentation>				</xs:annotation>				<xs:simpleType>					<xs:restriction base="xs:positiveInteger">						<xs:totalDigits value="4"/>						<xs:maxInclusive value="9999"/>					</xs:restriction>				</xs:simpleType>			</xs:element>		</xs:sequence>	</xs:complexType>	<xs:element name="datos">		<xs:annotation>			<xs:documentation>Datos del Documento de Transporte.</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:attribute name="ftra" type="xs:date" use="required">				<xs:annotation>					<xs:documentation>Fecha documento de transporte correspondiente a la factura por compra de mercancías que se introducen a Zona Franca del resto del mundo.</xs:documentation>				</xs:annotation>			</xs:attribute>		</xs:complexType>	</xs:element></xs:schema>